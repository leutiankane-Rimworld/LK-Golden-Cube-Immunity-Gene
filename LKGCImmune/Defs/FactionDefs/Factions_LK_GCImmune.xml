<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<FactionDef ParentName="FactionBase">
		<defName>LK_GCImmune</defName>
		<label>Anti Cube Consortium</label>
		<description>A group of former cultists that have grown immune to the machinations of the Golden Cube.</description>
		<pawnSingular>Consotium member</pawnSingular>
		<pawnsPlural>Consortium members</pawnsPlural>
		<categoryTag>Outlander</categoryTag>
		<settlementGenerationWeight>0.3</settlementGenerationWeight>
		<requiredCountAtGameStart>1</requiredCountAtGameStart>
		<earliestRaidDays>60</earliestRaidDays>
    	<canMakeRandomly>true</canMakeRandomly>
		<canSiege>true</canSiege>
		<canStageAttacks>true</canStageAttacks>
    	<leaderTitle>Anti Cubist Priest</leaderTitle>
		<permanentEnemy>true</permanentEnemy>
		<permanentEnemyToEveryoneExceptPlayer>false</permanentEnemyToEveryoneExceptPlayer>
		<fixedName>Anti Cube Consortium</fixedName>
    	<settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
		<techLevel>Ultra</techLevel>
		<fixedLeaderKinds>
		<li>LK_GCImmune_Royal_Stellarch</li>
		</fixedLeaderKinds>
		<visitorTraderKinds>
		<li>Orbital_BulkGoods</li>
       <li>Orbital_CombatSupplier</li>
       <li>Orbital_Exotic</li>
       <li>Orbital_PirateMerchant</li>
	   </visitorTraderKinds>
		<caravanTraderKinds>
			<li>Orbital_BulkGoods</li>
      <li>Orbital_CombatSupplier</li>
      <li>Orbital_Exotic</li>
      <li>Orbital_PirateMerchant</li>
		</caravanTraderKinds>
		<baseTraderKinds>
			<li>Orbital_BulkGoods</li>
		</baseTraderKinds>
		<allowedArrivalTemperatureRange>
		  <min>-1000</min>
		  <max>1000</max>
		</allowedArrivalTemperatureRange>
		<maxPawnCostPerTotalPointsCurve>
			<points>
				<li>(0,35)</li>
				<li>(70, 50)</li>
				<li>(700, 100)</li>
				<li>(1300, 150)</li>
				<li>(100000, 10000)</li>
			</points>
		</maxPawnCostPerTotalPointsCurve>
		<pawnGroupMakers Inherit="False">
			<li>
				<kindDef>Combat</kindDef>
				<options>
					<LK_GCImmune_Leader>1</LK_GCImmune_Leader>
					<LK_GCImmune_Officer>2</LK_GCImmune_Officer>
					<LK_GCImmune_Trooper>20</LK_GCImmune_Trooper>
					<LK_GCImmune_Trooper_Melee>10</LK_GCImmune_Trooper_Melee>
				</options>
			</li>
			<li>
				<kindDef>Peaceful</kindDef>
				<options>
					<LK_GCImmune_Leader>1</LK_GCImmune_Leader>
					<LK_GCImmune_Officer>2</LK_GCImmune_Officer>
					<LK_GCImmune_Trooper>20</LK_GCImmune_Trooper>
					<LK_GCImmune_Trooper_Melee>10</LK_GCImmune_Trooper_Melee>
				</options>
			</li>
			<li>
				<kindDef>Trader</kindDef>
				<traders>
					<LK_GCImmune_Trader>1</LK_GCImmune_Trader>
				</traders>
				<carriers>
					<Muffalo>4</Muffalo>
				</carriers>
				<guards>
					<LK_GCImmune_Officer>1</LK_GCImmune_Officer>
					<LK_GCImmune_Trooper>10</LK_GCImmune_Trooper>
					<LK_GCImmune_Trooper_Melee>10</LK_GCImmune_Trooper_Melee>
				</guards>
			</li>
			<li>
				<kindDef>Settlement</kindDef>
				<options>
				    <LK_GCImmune_Royal_Stellarch>1</LK_GCImmune_Royal_Stellarch>
					<LK_GCImmune_Leader>3</LK_GCImmune_Leader>
					<LK_GCImmune_Officer>3</LK_GCImmune_Officer>
					<LK_GCImmune_Trooper>20</LK_GCImmune_Trooper>
					<LK_GCImmune_Trooper_Melee>10</LK_GCImmune_Trooper_Melee>
					<LK_GCImmune_Citizen>5</LK_GCImmune_Citizen>
				</options>
			</li>
		</pawnGroupMakers>
    	<settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
		<factionIconPath>World/WorldObjects/Expanding/PirateOutpost</factionIconPath>
        <colorSpectrum>
            <li>RGB(179, 61, 35)</li>
        </colorSpectrum>
		<allowedCultures>
			<li>Astropolitan</li>
		</allowedCultures>
		<styles>
      <li MayRequire="Ludeon.RimWorld.Ideology">Techist</li>
    </styles>
		<requiredMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Structure_Ideological</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Raider</li>
		</requiredMemes>
		<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Structure_Animist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">AnimalPersonhood</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">TreeConnection</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Tunneler</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Darkness</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Rancher</li>
      <li MayRequire="Ludeon.RimWorld.Ideology,Ludeon.RimWorld.Biotech">Bloodfeeding</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Cannibal</li>
		</disallowedMemes>
		<structureMemeWeights>
      <Structure_TheistEmbodied MayRequire="Ludeon.RimWorld.Ideology">1</Structure_TheistEmbodied>
      <Structure_TheistAbstract MayRequire="Ludeon.RimWorld.Ideology">2</Structure_TheistAbstract>
      <Structure_Ideological MayRequire="Ludeon.RimWorld.Ideology">1</Structure_Ideological>
      <Structure_Archist MayRequire="Ludeon.RimWorld.Ideology">10</Structure_Archist>
    </structureMemeWeights>
		<xenotypeSet>
      <xenotypeChances>
        <LK_GCImmune MayRequire="Ludeon.RimWorld.Biotech">999.00</LK_GCImmune>  
      </xenotypeChances>
    </xenotypeSet>
		<maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
		<configurationListOrderPriority>30</configurationListOrderPriority>
	</FactionDef>

